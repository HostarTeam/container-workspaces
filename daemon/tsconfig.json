{
    "compilerOptions": {
        "target": "ES5",
        "rootDir": "src/",
        "outDir": "dist/",
        "importHelpers": false,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "importsNotUsedAsValues": "remove",
        "removeComments": true,
        "lib": ["ES2021.String"]
    }
}
