    "dependencies": {
        "@prisma/client": "^4.3.1",
        "bcryptjs": "^2.4.3",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "express": "^4.17.2",
        "http-proxy": "^1.18.1",
        "is-valid-hostname": "^1.0.2",
        "log4js": "^6.3.0",
        "mysql2": "^3.9.4",
        "node-fetch": "^2.6.6",
        "socket.io": "^4.5.2",
        "ws": "^8.4.0",
