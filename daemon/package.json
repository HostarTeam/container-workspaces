        "@typescript-eslint/parser": "^5.11.0",
        "eslint": "^8.21.0",
        "jest": "^29.2.2",
        "nodemon": "^2.0.20",
        "pkg": "^5.3.0",
        "prisma": "^4.3.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
